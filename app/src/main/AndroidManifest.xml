<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.First_App">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/UFF"
        android:supportsRtl="true"
        android:theme="@style/Theme.UFF">
        <activity
            android:name="com.example.First_App.view.MoviesDetailsActivity"
            android:exported="false" />
        <activity
            android:name="com.example.First_App.view.MoviesDetailsActivity"
            android:exported="false"
            tools:ignore="DuplicateActivity" />
        <activity
            android:name="com.example.First_App.view.MoviesActivity"
            android:exported="false" />
        <activity
            android:name="com.example.First_App.view.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>